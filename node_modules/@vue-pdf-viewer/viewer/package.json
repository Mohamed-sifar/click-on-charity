{
  "name": "@vue-pdf-viewer/viewer",
  "private": false,
  "version": "1.1.0",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "description": "The PDF Viewer component for Vue 3 and Nuxt",
  "keywords": [
    "component",
    "frontend",
    "frontend tools",
    "pdf",
    "pdf component",
    "pdf viewer",
    "pdfjs",
    "pdfjs-dist",
    "vue",
    "vue component",
    "vue pdf",
    "vue pdf viewer",
    "web component"
  ],
  "author": "Vue PDF Viewer <david@vue-pdf-viewer.dev>",
  "license": "See license section",
  "homepage": "https://vue-pdf-viewer.dev",
  "url": "https://github.com/vue-pdf-viewer/vue-pdf-viewer",
  "bugs": {
    "url": "https://github.com/vue-pdf-viewer/vue-pdf-viewer/issues/new"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/index.essential.mjs": {
      "import": "./dist/index.essential.mjs",
      "types": "./dist/types/index.essential.d.ts"
    },
    "./dist/style/index.css": {
      "import": "./dist/style/index.css",
      "require": "./dist/style/index.css"
    }
  },
  "style": "./dist/style/index.css",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "format": "prettier --write .",
    "prebuild": "vitest run",
    "postbuild": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "prepublishOnly": "bun run build",
    "shadcn:add": "bunx shadcn-vue@latest add",
    "prepare": "husky"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "dayjs": "^1.11.11",
    "pdfjs-dist": "4.4.168",
    "vite-plugin-css-injected-by-js": "^3.5.1"
  },
  "peerDependencies": {
    "pdfjs-dist": "^4.4.168",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@rollup/rollup-darwin-x64": "^4.19.1",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@unocss/reset": "^0.61.7",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitest/browser": "^1.5.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.25.0",
    "happy-dom": "^14.7.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.5",
    "lucide-vue-next": "^0.367.0",
    "prettier": "3.2.5",
    "radix-vue": "^1.6.2",
    "sass": "^1.75.0",
    "shadcn-vue": "^0.10.3",
    "tailwind-merge": "^2.2.2",
    "typescript": "^4.6.4",
    "unocss": "^0.59.0",
    "unocss-preset-animations": "^1.0.2",
    "unocss-preset-shadcn": "^0.2.3",
    "vite": "^5.2.8",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.4.0",
    "vue": "^3.2.37",
    "vue-tsc": "^0.40.4",
    "webdriverio": "^8.36.0"
  },
  "engines": {
    "node": ">=18.20.3",
    "npm": ">=9.5.1",
    "bun": ">=1.0.4",
    "pnpm": ">=8.8.0",
    "yarn": ">=1.20.0"
  }
}
